<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Estate Agency Web</title>
    <link rel="stylesheet" href="~/css/site.css" />
</head>
<body onload="page_main()">
    <header>
    </header>
    <main role="main">
        @RenderBody()
    </main>

    <footer class="footer container60">
        <div>
            &copy; 2021 - EstateAgencyWeb
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/jquery-ajax-unobtrusive/dist/jquery.unobtrusive-ajax.min.js"></script>
    <script src="~/js/site.js"></script>
    <script src="~/js/site-@(ViewData["Title"].ToString().ToLower()+".js")"></script>
</body>
</html>
